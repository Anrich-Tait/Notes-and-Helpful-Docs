#include <stdio.h>

int main() {

  int a, b, c, max;

  printf("Type a number: \n");
  scanf("%d", &a);

  printf("Type a number: \n");
  scanf("%d", &b);

  printf("Type a number: \n");
  scanf("%d", &c);

  if (a >= b && a >= c) {
    max = a;
  } else if (b >=a && b >= c) {
    max = b;
  } else {
    max = c;
  }
  printf("The highest number is: %d", max);
  return 0;
}
